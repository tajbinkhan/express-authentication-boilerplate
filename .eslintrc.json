{
	"plugins": ["unused-imports"],
	"rules": {
		"no-console": 0,
		"indent": 0,
		"linebreak-style": 0,
		"unused-imports/no-unused-imports": "warn", // Automatically remove unused imports
		"unused-imports/no-unused-vars": [
			"off",
			{
				"vars": "all",
				"varsIgnorePattern": "^_",
				"args": "after-used",
				"argsIgnorePattern": "^_"
			}
		]
	}
}
